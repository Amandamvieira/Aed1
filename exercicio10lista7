// exercício 10 lista 07
//Faça uma função que receba dois numeros e retorne o maior deles

#include <stdio.h>
int main(){
int v1,v2;

printf("valor 1 :"); scanf("%d",&v1);
printf("valor 2 :"); scanf("%d",&v2);

if (v1>v2){
    printf("O valor maior é %d",v1);
}
if (v1<v2){
    printf("o valor maior é %d", v2);
}
return(0);
}
